<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.vikorecipes.vikorecipes.entity.UsersEntity" table="users" schema="public" catalog="ViKoReciPeS">
        <id name="id">
            <column name="id" sql-type="integer"/>
        </id>
        <property name="login">
            <column name="login" sql-type="text"/>
        </property>
        <property name="userPassword">
            <column name="user_password" sql-type="varchar"/>
        </property>
        <property name="email">
            <column name="email" sql-type="text"/>
        </property>
        <property name="nickname">
            <column name="nickname" sql-type="text"/>
        </property>
        <property name="userRole">
            <column name="user_role" sql-type="text"/>
        </property>
    </class>
</hibernate-mapping>