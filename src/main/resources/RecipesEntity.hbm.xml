<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.vikorecipes.vikorecipes.entity.RecipesEntity" table="recipes" schema="public"
           catalog="ViKoReciPeS">
        <id name="id">
            <column name="id" sql-type="integer"/>
        </id>
        <property name="title">
            <column name="title" sql-type="text"/>
        </property>
        <property name="recipes">
            <column name="recipes" sql-type="text"/>
        </property>
    </class>
</hibernate-mapping>