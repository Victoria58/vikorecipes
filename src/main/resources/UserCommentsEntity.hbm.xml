<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.vikorecipes.vikorecipes.entity.UserCommentsEntity" table="user_comments" schema="public"
           catalog="ViKoReciPeS">
        <id name="id">
            <column name="id" sql-type="integer"/>
        </id>
        <property name="textComment">
            <column name="text_comment" sql-type="text"/>
        </property>
        <property name="dateComment">
            <column name="date_comment" sql-type="time"/>
        </property>
    </class>
</hibernate-mapping>